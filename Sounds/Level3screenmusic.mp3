			if (json['success']) {
				$('#notification').html('<div class="success" style="display: none;">' + json['success'] + '<img src="image/close.png" alt="" class="close" /></div>');
					
				$('.success').fadeIn('slow');
				$('#cart').fadeIn();	
				$('#cart-total').html(json['total']);
				
				$('html, body').animate({ scrollTop: 0 }, 'slow'); 
			}	
		}
	});
  }

